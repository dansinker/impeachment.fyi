---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml" />
    {% for new in site.data.news.news %}
      <item>
        <title>Impeachment latest: {{ new.date | date: "%B %-d, %Y" }}</title>
        <description><p>Where we're at: {{ new.yesterday | xml_escape }}</p>
        <p>Today:</p>
        <ul>
         {% for today in new.todays %}
          <li>{{ today.item }}</li>
        {% endfor %}
        </ul>
        <p><b>Watch to Watch For:</b> {{ new.tomorrow }}</p>
        </description>
        <pubDate>{{ new.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>{{ site.url }}/archive#{{ new.date | date: "%Y-%m-%d" }}</link>
        <guid isPermaLink="true">{{ site.url }}/archive#{{ new.date | date: "%Y-%m-%d" }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>